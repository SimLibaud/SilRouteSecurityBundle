{
    "name": "sil/route-security-bundle",
    "type": "symfony-bundle",
    "description": "Provide a way to secure accesses to all routes of an symfony application.",
    "keywords": ["symfony", "bundle", "security", "access", "control", "route", "role"],
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Libaud",
            "email": "simonlibaud@gmail.com"
        }
    ],
    "require": {
        "php": "~5.6 || ~7.0 || ~8.0",
        "symfony/framework-bundle": "^2.7 || ^3.0 || ^4.0 || ^5.0",
        "symfony/security": "^2.7 || ^3.0 || ^4.0 || ^5.0",
        "symfony/yaml": "^2.7 || ^3.0 || ^4.0 || ^5.0",
        "symfony/expression-language": "^2.7 || ^3.0 || ^4.0 || ^5.0",
        "twig/twig": "^1.28 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Sil\\RouteSecurityBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
