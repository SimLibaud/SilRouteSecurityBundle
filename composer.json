{
    "name": "sil/route-security-bundle",
    "type": "symfony-bundle",
    "description": "Provide a way to secure accesses to all routes of an symfony application.",
    "keywords": ["symfony", "bundle", "security", "access", "control", "route", "role"],
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Libaud",
            "email": "simonlibaud@gmail.com"
        }
    ],
    "require": {
        "php": "~7.2",
        "symfony/framework-bundle": "^5.0",
        "symfony/security-core": "^5.0",
        "symfony/yaml": "^5.0",
        "symfony/expression-language": "^5.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Sil\\RouteSecurityBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    }
}
